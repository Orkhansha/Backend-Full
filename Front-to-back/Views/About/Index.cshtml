<section id="textes">
    <div class="row my-5">
        <div class="col-lg-6 col-md-12 col-sm-12">
            <div class="text-left">
                <img src="~/assets/img/img-about/about-contact.30be8aa0.jpg" alt="">
            </div>
        </div>
        <div class="col-lg-6 col-md-12 col-sm-12">
