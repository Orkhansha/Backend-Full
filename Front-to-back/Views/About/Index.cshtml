                <div class="Contact-us">
                    <h1 class="modal-title">Write To Us</h1>

                    <div class="form-wrapper">

                        <form action="" class="form">
                            <div class="form-group">
                                <label class="form-label" for="first">First Name</label>
                                <input id="first" class="form-input" type="text" />
                            </div>
                            <div class="form-group">
                                <label class="form-label" for="first">Last Name</label>
                                <input id="first" class="form-input" type="text" />
                            </div>
                            <div class="form-group">
                                <label class="form-label" for="last">Email</label>
                                <input id="last" class="form-input" type="email" />
                            </div>

                            <div class="form-group">
                                <label class="form-label" for="last">Subject</label>
                                <input id="last" class="form-input" type="text" />
                            </div>
                            <div class="form-group">
                                <label class="form-label" for="color">Leave A Message</label>
                                <input id="color" class="form-input" type="text" />
                            </div>
                        </form>
                    </div>
                    <div class="modal-buttons">
                        <button type="button" class="btn btn-outline-success contact-us-btn">Send Message</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

